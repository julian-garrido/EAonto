<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:dol="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns="http://www.owl-ontologies.com/Ontology1321454521.owl#"
  xml:base="http://www.owl-ontologies.com/Ontology1321454521.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="NaturalSuperficialWater">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SuperficialWater"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CivilProtection">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Agent"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="useAndNeed"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="FloodWave"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#useAndNeed"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:ID="DischargeDepth"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SingleAtmosphericHazard">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="AtmosphericHazard"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="es">Elementos atmosféricos sencillos.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Device">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-object"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Weir">
    <rdfs:label xml:lang="en">weir</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SoilTransformationAndConstruction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EmbankmentBreak">
    <rdfs:label xml:lang="en">embankment break</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="StructuralHazard"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Forecasting">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="AssessmentReportOrProcedure"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">forecasting</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SuperficialWaterDischargeFlow">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="WaterDischarge"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="dischargeAffectedBy"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="CatchmentArea"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#CatchmentArea"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#dischargeAffectedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasCharacterizingIndicator"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#DischargeDepth"/>
              <owl:Class rdf:ID="DischargeVelocity"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#DischargeVelocity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasCharacterizingIndicator"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#hasCharacterizingIndicator"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#DischargeDepth"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="WaterRegulation"/>
              <owl:Class rdf:about="#NaturalSuperficialWater"/>
              <owl:Class rdf:ID="ArtificialWaters"/>
              <owl:Class rdf:ID="Rainfall"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="dischargeProducedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Discharge of the superficial water flow</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="HydraulicHazardAssessment">
    <rdfs:label xml:lang="en">hydraulic hazard assessment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AssessmentReportOrProcedure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RainfallDepth">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="RainfallAnalisysParameter"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isObtainedWith"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class rdf:ID="RainDevice"/>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RainDevice"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AtmosphericHazard">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Peligros originados por fenómenos atmosféricos.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="NaturalProcess"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#RainDevice">
    <rdfs:label xml:lang="en">device to measure rain data</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Device"/>
  </owl:Class>
  <owl:Class rdf:about="#WaterRegulation">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="TrafficChanges"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:ID="BypassWeir"/>
          <owl:Class rdf:about="#Weir"/>
          <owl:Class rdf:ID="ArtificialCanal"/>
          <owl:Class rdf:ID="Dam"/>
          <owl:Class rdf:ID="Embankments"/>
          <owl:Class rdf:ID="StorageTank"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="HistoricalRainfallSeries">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DataSource"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#RainDevice"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Actions">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ImpactingActions"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">Based on actions of Leopold matrix.</rdfs:comment>
    <rdfs:comment xml:lang="es">Basado en las acciones según la matriz de Leopold.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="HazardMap">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Model"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RainfallModel">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Model"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ProtectionOfGoods">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="MitigatingAction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SyntethicHistoricalRainfallSeries">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="RainfallStatisticalAnalisys"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#RainDevice"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EnvironmentalFactor">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Elementos del medio susceptibles de impacto</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#physical-endurant"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="#SuperficialWater">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Water"/>
    </rdfs:subClassOf>
    <rdfs:comment xml:lang="en">means inland waters, except groundwater; transitional waters and coastal waters, except in respect of chemical status for which it shall also include territorial waters</rdfs:comment>
    <rdfs:comment xml:lang="es">Clasificada de acuerdo con la directiva europea 200/60/CE: las aguas continentales, excepto las aguas subterráneas; las aguas de transición y las aguas costeras, y, en lo que se refiere al estado químico, también las aguas territoriales</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#FloodWave">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Model"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Evacuation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#MitigatingAction"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="actionManagedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#CivilProtection"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#MitigatingAction">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Management"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">mitigating action</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="HydrologicalHazardAssessment">
    <rdfs:label xml:lang="en">hydrological hazard assessment</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#AssessmentReportOrProcedure"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="TechnologicHazard">
    <rdfs:comment xml:lang="es">Peligros originados por accidentes derivados de la tecnología</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NaturalProcess"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="HydrologicalHazard">
    <rdfs:comment xml:lang="es">Peligros originados en los fenómenos meteorológicos</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#NaturalProcess"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Water">
    <rdfs:comment xml:lang="es">Agua</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#EnvironmentalFactor"/>
  </owl:Class>
  <owl:Class rdf:ID="PublicInvolvement">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#accomplishment"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label xml:lang="en">public involvement</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="CatchmentTotalArea">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="CatchmentIndicator"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">catchment area</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#StorageTank">
    <rdfs:label xml:lang="en">water storage tank</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SoilTransformationAndConstruction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="IDF">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#RainfallStatisticalAnalisys"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="isParametrizedBy"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="RainfallIntensity"/>
              <owl:Class rdf:ID="RainfallDuration"/>
              <owl:Class rdf:ID="RainfallFrequency"/>
              <owl:Class rdf:ID="ReturnPeriod"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RainfallFrequency"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isParametrizedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isParametrizedBy"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RainfallDuration"/>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="#RainfallIntensity"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isParametrizedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#RainfallIntensity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#RainfallAnalisysParameter"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
        <owl:allValuesFrom rdf:resource="#RainDevice"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#RainDevice"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RecoveryAction">
    <rdfs:label xml:lang="en">recovery action</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Management"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#CivilProtection"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#actionManagedBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#AssessmentReportOrProcedure">
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#accomplishment"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="#Embankments">
    <rdfs:label xml:lang="en">embankments</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Barrier"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="VulneravilityAssessment">
    <rdfs:subClassOf rdf:resource="#AssessmentReportOrProcedure"/>
    <rdfs:label xml:lang="en">vulnerativity assessment</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="TimeOfConcentration">
    <rdfs:label xml:lang="en">time of concentration</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#CatchmentIndicator"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Socioeconomic">
    <rdfs:comment xml:lang="es">Elementos socioeconómicos</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#EnvironmentalFactor"/>
  </owl:Class>
  <owl:Class rdf:about="#NaturalProcess">
    <rdfs:comment xml:lang="es">Procesos naturales</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ImpactingActions"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#BypassWeir">
    <rdfs:label xml:lang="en">bypass weir</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SoilTransformationAndConstruction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#ImpactingActions">
    <rdfs:comment xml:lang="es">Acciones o actividades que producen impacto</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#perdurant"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:about="#StructuralHazard">
    <rdfs:label xml:lang="en">structural hazard</rdfs:label>
    <rdfs:subClassOf rdf:resource="#TechnologicHazard"/>
  </owl:Class>
  <owl:Class rdf:about="#WaterDischarge">
    <rdfs:subClassOf rdf:resource="#HydrologicalHazard"/>
    <owl:equivalentClass>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SuperficialWaterDischargeFlow"/>
          <owl:Class rdf:ID="GroundWaterDischargeFlow"/>
        </owl:unionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#RainfallDuration">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#RainfallAnalisysParameter"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#RainDevice"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#isObtainedWith"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#RainDevice"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="EconomicEvaluation">
    <rdfs:subClassOf rdf:resource="#AssessmentReportOrProcedure"/>
    <rdfs:label xml:lang="en">economic evaluation</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="FreshwaterFlood">
    <rdfs:comment xml:lang="en">Flood due to rivers, lakes or broken dams</rdfs:comment>
    <rdfs:comment xml:lang="es">Inundación de agua dulce debida a ríos,lagos o rupturas de presas</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#achievement"/>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="rootBriefModel"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#SuperficialWaterDischargeFlow"/>
              <owl:Class rdf:ID="BridgeOcclusion"/>
              <owl:Class rdf:about="#EmbankmentBreak"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="floodProduceBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="TownPlanning"/>
              <owl:Class rdf:ID="EarlyWarning"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasPreventiveAction"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasMitigatingAction"/>
        </owl:onProperty>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:about="#Evacuation"/>
              <owl:Class rdf:ID="HousesProtection"/>
            </owl:unionOf>
          </owl:Class>
        </owl:someValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasRecoveryAction"/>
        </owl:onProperty>
        <owl:someValuesFrom rdf:resource="#RecoveryAction"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#CatchmentIndicator">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="IndicatorsAndMeasureUnits"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">catchment area indicator</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#DischargeVelocity">
    <rdfs:label xml:lang="en">discharge velocity</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="DischargeIndicator"/>
    </rdfs:subClassOf>
